{
    "exp_entry":{
        "action":"main_drive_confirm",
        "next":[
            "exp_enter",
            "exp_entry"
        ]
    },
    "exp_enter":{
        "desc":"说是进入 exp，不过这个坐标就是 luxcavation 的通用路线",
        "recognition":"template_match",
        "action":"click",
        "params":{
            "template":"inferno",
            "target":[
                440, 
                160
            ],
            "post_delay":2
        },
        "next":[
            "exp_select_stage"
        ]
    },
    "exp_select_stage":{
        "desc":"先检测是否有要找的stage（这个在共享变量中），有就做相应点击，没有就从左向右拖拽一页，然后再检测形成循环，直到找到。",
        "recognition":"template_match",
        "action":"exp_select_stage",
        "params":{
            "template":"luxcavation",
            "mode":"luxcavation_mode"
        },
        "next":[
            "exp_choose_team",
            "exp_skip_battle"
        ]
    }, 
    "exp_skip_battle":{
        "recognition":"template_match",
        "action":"click",
        "params":{
            "pre_delay":1,
            "target":[750, 490],
            "template":"skip_battle"
        },
        "next":[
            "exp_victory"
        ]
    },
    "exp_choose_team":{
        "recognition":"template_match",
        "action":"choose_team",
        "params":{
            "template":"details",
            "cfg_type":"exp"
        },
        "next":[
            "exp_ready_to_battle"
        ]
    },
    "exp_ready_to_battle":{
        "action":"ready_to_battle",
        "params":{
            "cfg_type":"exp"
        },
        "next":[
            "exp_battle"
        ]
    },
    "exp_battle":{
        "action":"battle_entry",
        "next":[
            "exp_victory",
            "exp_defeat",
            "exp_battle"
        ]
    },
    "exp_victory":{
        "recognition":"template_match",
        "action":"key",
        "params":{
            "template":"victory",
            "key":"enter"
        },
        "next":[
            "exp_check"
        ]
    },
    "exp_defeat":{
        "recognition":"template_match",
        "action":"key",
        "params":{
            "template":"defeat",
            "key":"enter"
        },
        "next":[
            "exp_select_stage"
        ]
    },
    "exp_check":{
        "type":"check",
        "params":{
            "origin":"exp_select_stage",
            "disable_node":"exp_entry",
            "execute_count": 0
        },
        "next":[
            "exp_quit"
        ]
    },
    "exp_quit":{
        "desc":"回到主界面，方便其它任务执行",
        "action":"key",
        "params":{
            "key":"esc"
        },
        "next":[
            "main_circle_center"
        ]
    },
    "thread_entry":{
        "action":"main_drive_confirm",
        "next":[
            "thread_enter",
            "thread_entry"
        ]
    },
    "thread_enter":{
        "desc":"除了名字，就是复用 enter exp 的",
        "recognition":"template_match",
        "action":"click",
        "params":{
            "template":"inferno",
            "target":[
                440, 
                160
            ],
            "post_delay":2
        },
        "next":[
            "thread_select_stage"
        ]
    },
    "thread_select_stage":{
        "recognition":"template_match",
        "action":"thread_select_stage",
        "params":{
            "template":"luxcavation",
            "mode": "luxcavation_mode"
        },
        "next":[
            "thread_choose_team",
            "thread_skip_battle"
        ]
    },
    "thread_skip_battle":{
        "recognition":"template_match",
        "action":"click",
        "params":{
            "pre_delay":1, 
            "target":[750, 490],
            "template":"skip_battle"
        },
        "next":[
            "thread_victory"
        ]
    },
    "thread_choose_team":{
        "recognition":"template_match",
        "action":"choose_team",
        "params":{
            "template":"details",
            "cfg_type":"thread"
        },
        "next":[
            "thread_ready_to_battle"
        ]
    },
    "thread_ready_to_battle":{
        "action":"ready_to_battle",
        "params":{
            "cfg_type":"thread"
        },
        "next":[
            "thread_battle"
        ]
    },
    "thread_circle_center":{
        "action":"battle_entry",
        "next":[
            "thread_victory",
            "thread_defeat",
            "thread_battle",
            "thread_event",
            "thread_circle_center"
        ]
    },
    "thread_battle":{
        "recognition":"template_match",
        "action":"battle_entry",
        "params":{
            "template":"owned_ego_resources"
        },
        "next":[
            "thread_circle_center"
        ]
    },
    "thread_event":{
        "recognition":"template_match",
        "action":"event_entry",
        "params":{
            "template":"event_skip"
        },
        "next":[
            "thread_circle_center"
        ]
    },
    "thread_victory":{
        "recognition":"template_match",
        "action":"key",
        "params":{
            "template":"victory",
            "key":"enter"
        },
        "next":[
            "thread_check"
        ]
    },
    "thread_defeat":{
        "recognition":"template_match",
        "action":"key",
        "params":{
            "template":"defeat",
            "key":"enter"
        },
        "next":[
            "thread_select_stage"
        ]
    },
    "thread_check":{
        "type":"check",
        "params":{
            "origin":"thread_select_stage",
            "disable_node":"thread_entry"
        },
        "next":[
            "thread_quit"
        ]
    },
    "thread_quit":{
        "desc":"回到主界面，方便其它任务执行",
        "action":"key",
        "params":{
            "key":"esc"
        },
        "next":[
            "main_circle_center"
        ]
    }
}